{
  "name": "sweetlime",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "deploy": "npm run -n \"-r dotenv/config\" && npm run build && gatsby-plugin-s3 deploy",
    "build": "gatsby build",
    "develop": "env-cmd -f .env gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.1",
    "gatsby": "^2.20.12",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-canonical-urls": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.2.5",
    "gatsby-plugin-s3": "^0.3.4",
    "gatsby-plugin-sass": "^2.2.3",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-remark-images": "^3.2.4",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-contentful": "^2.3.36",
    "gatsby-source-filesystem": "^2.2.4",
    "gatsby-transformer-remark": "^2.7.3",
    "gatsby-transformer-sharp": "^2.5.12",
    "node-sass": "^4.14.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "gatsby-source-graphql": "^2.4.3",
    "prettier": "2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
